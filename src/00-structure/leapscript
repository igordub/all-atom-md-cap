source leaprc.protein.ff14SB
source leaprc.gaff
source leaprc.water.tip3p 
loadamberparams frcmod.parmbsc1
loadamberparams ligand.frcmod

PRO = loadpdb pdb/processed/protein.pdb
LIG = loadmol2 pdb/processed/ligand.mol2
COM = combine {PROT LIG}
check LIG

set default PBradii mbondi3
solvateOct mol TIP3PBOX 8
addions mol Cl- 1

saveoff LIG ligand.lib
saveamberparm PRO protein.prmtop protein.inpcrd
saveamberparm LIG ligand.prmtop ligand.inpcrd
saveamberparm COM complex.prmtop complex.inpcrd
savepdb complex.pdb
quit